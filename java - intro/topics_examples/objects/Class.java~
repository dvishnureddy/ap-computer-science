/**
 * Simulates the grades of a class
 * @author Ryan O'Hara
 * @Version 1.0
 */
public class Class
{
     private double Grade;
     
     /**
      * @param initGrade
      */
     
     public Class (double initGrade)
     {
          Grade = initGrade;
     }
     
     /**
      * @ param score
      * @ param initGrade
      */
     
     public void test (double score, initGrade)
     {
          Grade = initGrade;
          Grade += score;
          Grade = (initGrade + score) /2;
     }
     
     /**
      * @ param score
      */
     
     public void project (double score)
     {
          Grade += score;
     }
     
     /**
      * @ return the grade
      */
     
     public double checkGrade ()
     {
          return Grade;
     }
     
}//end Class class
     
            
          
          
          