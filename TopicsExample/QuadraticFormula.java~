public class QuadraticFormula
{
     public static void main (String args[])
     {
          double a = 2;
          double b = 3;
          double c = 5;
          
          double root = Math.sqrt (Math.pow (b,2) - 4*a*c);
          
          double x = (-1 * b + root)/(2*a);
          double x2 = (-1 * b - root)/(2*a);
               
          
               
     }//end main method
}//end ClassName class