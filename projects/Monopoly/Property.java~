public class Property
{
     private String name;
     private int owner;
     private int type;
     private String color;
     private int cost;
     private int rent;
     private int morgage;
     private int space; 
          
     public Property (String n, int own, int t, String col, int cos, int ren, int mort, int spa)
     {
          name = n;
          owner = own;
          type = t;
          color = col;
          cost = cos;
          rent = ren;
          morgage = mort;
          space = spa;
        
          
     }
     
     public String getName()
     {
          return name;
     }
     
     public String getOwner()
     {
          return owner;
     }
     
     public void setOwner(int owner)
     {
          set owner;
     }
     public Sting getType() 
     {
          return type;
     }
     
     public String getColor()
     {
          return color;
     }
     
     public String getCost()
     {
          return cost;
     }
     
     public String getRent()
     {
          return rent;
     }
     
     public String getMortgage()
     {
          return mortgage;
     }
     
     public String getSpace()
     {
          return space;
     }
     
     public String toString()
     {
          return "name: " + name
             + "\nowner: " + owner 
             + "\ntype: " + type
             + "\ncolor: " + color
             + "\ncost: " + cost
             + "\nrent: " + rent
             + "\nspace: " + space
     }
     
     }
     
     